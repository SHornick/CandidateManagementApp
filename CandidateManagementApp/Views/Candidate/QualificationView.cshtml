@model CandidateManagementApp.Models.Qualification


@using (Html.BeginCollectionItem("Qualifications"))
{
    <!-- Adds a new set of Qualifications to fill -->
    <div class="form-group">
        <label asp-for="QualificationType" class="control-label"></label>
        <select asp-for="QualificationType" asp-items="Html.GetEnumSelectList<QualificationType>()" class="form-control"></select>
    </div>
    <div class="form-group">
        <label asp-for="Name" class="control-label"></label>
        <input asp-for="Name" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="DateStarted" class="control-label"></label>
        <input asp-for="DateStarted" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="DateCompleted" class="control-label"></label>
        <input asp-for="DateCompleted" class="form-control" />
    </div>
}